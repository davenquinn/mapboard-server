{
  "name": "mapboard-server",
  "version": "2.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "bin": {
    "mapboard": "dist/cli.js"
  },
  "engines": {
    "node": "12.x || 14.x"
  },
  "source": [
    "src/index.ts",
    "src/cli.ts",
    "src/test-runner.ts"
  ],
  "targets": {
    "main": false,
    "node": {
      "context": "node"
    }
  },
  "scripts": {
    "prepare": "npm run build",
    "clean": "rm -rf dist .parcel-cache",
    "build": "parcel build --target node src/index.ts src/cli.ts src/test-runner.ts",
    "dev": "parcel watch src/index.ts src/cli.ts src/test-runner.ts",
    "compile": "./compile",
    "test": "ava"
  },
  "dependencies": {
    "@mapbox/mbtiles": "^0.12.1",
    "@mapbox/tilelive": "^6.1.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.17.1",
    "coffeescript": "^2.5.1",
    "colors": "^1.1.2",
    "cors": "^2.8.4",
    "express": "^4.15.2",
    "express-list-routes": "^0.1.4",
    "glob": "^7.1.6",
    "morgan": "^1.9.0",
    "pg-promise": "^10.7.3",
    "regenerator-runtime": "^0.13.7",
    "response-time": "^2.3.2",
    "socket.io": "^2.3.0",
    "tessera": "^0.14.1",
    "tilelive-cache": "^0.7.1",
    "tilelive-modules": "^0.4.2",
    "wkx": "^0.4.1",
    "yargs": "^16.2.0"
  },
  "pkg": {
    "assets": [
      "sql/**/*.sql",
      "node_modules/@mapbox/mbtiles/lib/**/*.*"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@parcel/babel-preset-env": "^2.0.0-nightly.537+49ac2175",
    "ava": "^3.15.0",
    "parcel": "^2.0.0-beta.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
